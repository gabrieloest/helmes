CREATE DATABASE helmes;

USE helmes;

-- Table: public.users

-- DROP TABLE public.users;

CREATE TABLE public.users
(
  id bigint NOT NULL,
  name character varying(255) NOT NULL,
  subscription_id bigint,
  CONSTRAINT users_pkey PRIMARY KEY (id),
  CONSTRAINT fkfwx079xww5uyfbpi9u8gwam34 FOREIGN KEY (subscription_id)
      REFERENCES public.subscriptions (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION
)
WITH (
  OIDS=FALSE
);
ALTER TABLE public.users
  OWNER TO postgres;


-- Table: public.sectors

-- DROP TABLE public.sectors;

CREATE TABLE public.sectors
(
  id bigint NOT NULL,
  description character varying(255) NOT NULL,
  father_id bigint,
  CONSTRAINT sectors_pkey PRIMARY KEY (id),
  CONSTRAINT fk998tkre9k23s6bxbawlra1evl FOREIGN KEY (father_id)
      REFERENCES public.sectors (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION
)
WITH (
  OIDS=FALSE
);
ALTER TABLE public.sectors
  OWNER TO postgres;


 -- Table: public.subscriptions

-- DROP TABLE public.subscriptions;

CREATE TABLE public.subscriptions
(
  id bigint NOT NULL,
  term_agreement boolean NOT NULL,
  CONSTRAINT subscriptions_pkey PRIMARY KEY (id)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE public.subscriptions
  OWNER TO postgres;


-- Table: public.subscription_sector

-- DROP TABLE public.subscription_sector;

CREATE TABLE public.subscription_sector
(
  subscription_id bigint NOT NULL,
  sector_id bigint NOT NULL,
  CONSTRAINT subscription_sector_pkey PRIMARY KEY (subscription_id, sector_id),
  CONSTRAINT fkf3qw22imqwe4ouap0fyyanbmk FOREIGN KEY (sector_id)
      REFERENCES public.sectors (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION,
  CONSTRAINT fkj71p0e45fo70imy7c7s00qxom FOREIGN KEY (subscription_id)
      REFERENCES public.subscriptions (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION
)
WITH (
  OIDS=FALSE
);
ALTER TABLE public.subscription_sector
  OWNER TO postgres;


